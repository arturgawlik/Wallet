<script src="~/lib/jquery-validation/js/jquery.validate.min.js" type="text/javascript"></script> <script src="~/lib/bootstrap-wizard/jquery.bootstrap.wizard.min.js" type="text/javascript"></script> <script src="~/js/form-validation.js" type="text/javascript"></script>
</environment>
